{
  "name": "axios-auto",
  "version": "1.0.0",
  "description": "Simple Axios function with basic retry & tor support",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "types": "./types/index.d.ts",
  "browser": "./dist/browser/index.js",
  "scripts": {
    "start": "npm run lint && npm run build && npm run test",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "mocha test/index.js --timeout 500000",
    "test:browser": "karma start --single-run",
    "build": "npm run clean && npm run build:type && npm run build:node && npm run build:browser && npm run build:copy",
    "clean": "rimraf dist test",
    "build:type": "tsc -p tsconfig.json --outDir types --declaration --emitDeclarationOnly",
    "build:node": "rollup -c",
    "build:browser": "webpack",
    "build:copy": "node example/copyfile.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayanamitech/axios-auto.git"
  },
  "keywords": [
    "axios",
    "async",
    "get",
    "post",
    "fetch"
  ],
  "author": "AyanamiTech <ayanami0330@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayanamitech/axios-auto/issues"
  },
  "homepage": "https://github.com/ayanamitech/axios-auto#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "serialize-error-browser": "^9.1.1",
    "socks-proxy-agent": "^6.2.0"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "assert": "^2.0.0",
    "assertion-error": "^2.0.0",
    "axios-mock-adapter": "^1.20.0",
    "esbuild": "^0.14.42",
    "esbuild-loader": "^2.19.0",
    "eslint": "^8.16.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.0.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.3",
    "rollup-plugin-esbuild": "^4.9.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
